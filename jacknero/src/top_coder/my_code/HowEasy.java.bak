import java.util.*;
import java.util.regex.*;
import java.text.*;
import java.math.*;
import java.awt.geom.*;

public class HowEasy
{
	public int pointVal(String param0)
	{
		int tot = 0, wc = 0;
		String word;
		String[] tokens = param0.split(" ");
		for(int i = 0; i < tokens.length; i++){
			if(tokens[i].matches("^[a-zA-Z]+(\\.)?$")){
				word = tokens[i].replace(".","");
				tot += word.length();
				wc++;
			}
		}
		int awc = wc > 0 ? tot / wc : 0;
		return awc < 4 ? 250 : (awc >5 ? 1000 : 500);
	}
	
	
}
